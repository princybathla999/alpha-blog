<h1 align="center">Listing All Categories</h1>
<div align="center">
  <%= will_paginate %>
<%@categories.each do|category| %>
<ul class="listing">
<div class="row">

  <div class="col-xs-8 col-xs-offset-2">

    <div class="well well-lg">

      <div class="article-title">
<li>
        <%= link_to "#{category.name}", category_path(category) %>
</li>
      </div>
      </div>
      </div>
      </div>
</ul>

  <% end %>
  <%= will_paginate %>
</div>